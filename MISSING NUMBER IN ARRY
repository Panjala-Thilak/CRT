//Method 2 using XOR
#include <iostream>
using namespace std;

int main() {
	//code
	int t,n,i;
	cin>>t;
	while(t--)
	{
	    int x1=1,x2=1;
	    cin>>n;
	    int a[n];
	    for(i=0;i<n-1;i++)
	    cin>>a[i];
	    for(i=1;i<=n;i++)
	    x1=x1^i;
	     for(i=0;i<n-1;i++)
	     x2=x2^a[i];
	    cout<<(x1^x2)<<endl;
	   	}
	return 0;
}
